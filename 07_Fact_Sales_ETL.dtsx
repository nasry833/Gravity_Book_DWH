                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">/* Load Data to Fact_transcation Table*/
SELECT
  co.customer_id,
  co.shipping_method_id,
  ol.book_id,
  co.order_id,
  ol.line_id,
  co.dest_address_id,
  ast.address_status,
  CAST(CONVERT(varchar, pvt.Received, 112) AS int) AS Date_Received,
  CAST(CONVERT(varchar, pvt.PendingDelivery, 112) AS int) AS Date_PendingDelivery,
  CAST(CONVERT(varchar, pvt.InProgress, 112) AS int) AS Date_InProgress,
  CAST(CONVERT(varchar, pvt.Deliverd, 112) AS int) AS Date_Deliverd,
  CAST(CONVERT(varchar, pvt.Cancelled, 112) AS int) AS Date_Cancelled,
  CAST(CONVERT(varchar, pvt.Returned, 112) AS int) AS Date_Returned,
  ol.price,
  pvt.history_id
FROM cust_order co
LEFT JOIN order_line ol ON co.order_id = ol.order_id
inner JOIN (
    SELECT
      oh.order_id,
	  MAX(oh.history_id) AS history_id,
      MAX(CASE WHEN os.status_value = 'Order Received' THEN oh.status_date ELSE NULL END) AS Received,
      MAX(CASE WHEN os.status_value = 'Pending Delivery' THEN oh.status_date ELSE NULL END) AS PendingDelivery,
      MAX(CASE WHEN os.status_value = 'Delivery In Progress' THEN oh.status_date ELSE NULL END) AS InProgress,
      MAX(CASE WHEN os.status_value = 'Delivered' THEN oh.status_date ELSE NULL END) AS Deliverd,
	  MAX(CASE WHEN os.status_value = 'Cancelled' THEN oh.status_date ELSE NULL END) AS Cancelled,
	  MAX(CASE WHEN os.status_value = 'Returned' THEN oh.status_date ELSE NULL END) AS Returned
    FROM order_history oh
    LEFT JOIN order_status os ON oh.status_id = os.status_id
    GROUP BY oh.order_id
) pvt ON co.order_id = pvt.order_id
LEFT JOIN shipping_method sm ON co.shipping_method_id = sm.method_id
LEFT JOIN customer_address ca ON co.customer_id = ca.customer_id and co.dest_address_id = ca.address_id
LEFT JOIN address_status ast ON ca.status_id = ast.status_id</property>
